{
  "name": "SvelteRazor",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn && npm i && dotnet build -v q && npm run build:svelte",
    "dev": "dotnet watch run --launch-profile hotreloadprofile --urls http://127.1.1.0:8080",
    "build:svelte": "rollup -c rollup.config.js"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "rollup": "^2.39.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.32.3",
    "tailwindcss": "^3.4.1"
  }
}
